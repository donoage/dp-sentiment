{
  "name": "dp-sentiment",
  "version": "1.0.0",
  "description": "Dark pool sentiment tracker for SPY and QQQ top holdings",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "format": "prettier --write \"**/*.{js,json,css,html}\"",
    "format:check": "prettier --check \"**/*.{js,json,css,html}\"",
    "prepare": "husky"
  },
